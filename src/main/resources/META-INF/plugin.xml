<idea-plugin>
    <id>com.easternenterprise.decor</id>
    <name>Decor</name>
    <vendor url="https://github.com/easternenterprise">Eastern Enterprise</vendor>

    <description><![CDATA[
  Decor
  <br/><br/>
  To install:
  <ul>
  <li>Go to <i>Settings (Preferences) | Plugins</i>, find the theme plugin and install it</li>
  <li>Restart IDE</li>
  <li>Go to <i>Settings (Preferences) | Appearance & Behavior | Appearance</i> and see the <i>Theme</i> dropdown</li>
  </ul>
  <br/>

    ]]></description>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="191.0"/>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>


    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <themeProvider id="adf99807-aee9-4514-b512-5f0e429f30da" path="/Decor_Light.theme.json"/>
        <themeProvider id="eb9b7461-397b-4b98-a422-224fc0a74564" path="/Decor_Gray.theme.json"/>
        <themeProvider id="776bbe10-21be-477e-a5b3-5cb4403feab4" path="/Decor_Dark.theme.json"/>
    </extensions>

</idea-plugin>
